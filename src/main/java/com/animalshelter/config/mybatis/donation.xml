<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  <mapper namespace="com.animalshelter.donation.DonationDao">
  
  <update id="ddonation" parameterType="map">
	update donationboard
	set gatheringfee = gatheringfee +  #{money}
	where seq = #{seq}
	</update>
	</mapper>
	